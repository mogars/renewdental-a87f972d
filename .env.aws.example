# Additional environment variables for AWS migration
# Add these to your .env file when using AWS backend

# Set to 'true' to use AWS backend instead of Supabase
VITE_USE_AWS_BACKEND=false

# AWS Express Backend URL
VITE_AWS_API_URL=http://localhost:3001

# AWS Cognito Configuration
VITE_COGNITO_USER_POOL_ID=us-east-1_xxxxxxxxx
VITE_COGNITO_CLIENT_ID=xxxxxxxxxxxxxxxxxxxxxxxxxx
VITE_COGNITO_REGION=us-east-1
